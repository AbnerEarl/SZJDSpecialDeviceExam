<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_weight="1"
        android:text="检验结论：" />

    <TextView
        android:id="@+id/ro_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/textView50"
        android:layout_toRightOf="@+id/textView50"
        android:text="TextView"
        tools:text="斯蒂芬斯蒂芬\n" />

    <TextView
        android:id="@+id/ro_opinion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/ro_result"
        android:layout_toEndOf="@+id/textView50"
        android:layout_toRightOf="@+id/textView50"
        android:singleLine="true"
        android:text="TextView"
        tools:text="斯蒂芬斯蒂芬\n" />

    <TextView
        android:id="@+id/ro_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/ro_opinion"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignStart="@+id/ro_opinion"
        android:layout_below="@+id/ro_opinion"
        tools:text="斯蒂芬斯蒂芬\n" />

    <TextView
        android:id="@+id/textView113"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/ro_result"
        android:layout_marginLeft="10dp"
        android:text="检验意见：" />

    <TextView
        android:id="@+id/textView114"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/ro_status"
        android:layout_alignBottom="@+id/ro_status"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:text="审核状态：" />
</RelativeLayout>
