<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="1">

    <Button
        android:id="@+id/or_modify"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:focusable="false"
        android:layout_marginTop="3dp"
        android:text="修改" />

    <TextView
        android:id="@+id/or_resultt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="5dp"
        android:layout_toEndOf="@+id/textView116"
        android:layout_toLeftOf="@+id/or_modify"
        android:layout_toRightOf="@+id/textView116"
        android:layout_toStartOf="@+id/or_modify"
        android:ellipsize="none"
        android:maxLines="5"
        android:text="TextView"
        tools:text="信息\n" />

    <TextView
        android:id="@+id/or_opinionn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/or_resultt"
        android:layout_toEndOf="@+id/textView118"
        android:layout_toLeftOf="@+id/or_modify"
        android:layout_toRightOf="@+id/textView118"
        android:layout_toStartOf="@+id/or_modify"
        android:ellipsize="none"
        android:maxLines="5"
        android:singleLine="false"
        android:text="TextView"
        tools:text="信息\n" />

    <TextView
        android:id="@+id/or_statuss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/or_opinionn"
        android:layout_alignRight="@+id/or_opinionn"
        android:layout_below="@+id/or_opinionn"
        android:layout_toEndOf="@+id/textView122"
        android:layout_toRightOf="@+id/textView122"
        android:ellipsize="none"
        android:maxLines="5"
        android:text="TextView"
        tools:text="信息\n" />

    <TextView
        android:id="@+id/or_suggestionn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/or_statuss"
        android:layout_alignRight="@+id/or_statuss"
        android:layout_below="@+id/or_statuss"
        android:layout_marginBottom="5dp"
        android:layout_toEndOf="@+id/textView123"
        android:layout_toRightOf="@+id/textView123"
        android:ellipsize="none"
        android:maxLines="20"
        android:text="TextView"
        tools:text="信息\n" />

    <TextView
        android:id="@+id/textView116"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:text="检验结论："
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView118"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/or_opinionn"
        android:layout_marginLeft="10dp"
        android:text="检验意见："
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView122"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/or_statuss"
        android:layout_alignBottom="@+id/or_statuss"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="10dp"
        android:text="审核状态："
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView123"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/or_statuss"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="10dp"
        android:text="修改意见："
        android:textStyle="bold" />

</RelativeLayout>
